{
  "expo": {
    "name": "Strongwill",
    "description": "A gentile habit tracker",
    "slug": "strongwill-app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios"],
    "android": {
      "package": "com.javonharper.strongwill"
    },
    "ios": {
      "bundleIdentifier": "com.javonharper.strongwill",
      "supportsTablet": true
    },
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "strongwill",
            "project": "strongwill-app",
            "authToken": "cb6211acf3b841c0ad6c2963fd66e4eb4b3999c21def49279387338a4c632bf2"
          }
        }
      ]
    }
  }
}
